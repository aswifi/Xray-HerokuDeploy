{
  "name": "Deploy Xray Vless to Heroku",
  "logo": "https://github.com/aswifi/Xray-heroku/raw/main/logo.png",
  "description": "Deploy Xray Vless to Heroku.",
  "keywords": ["V2ray", "Xray"],
  "env": {
    "AppName": {
      "description": "请输入最上方填写的App Name，用于生成客户端配置和二维码，如果不想生成输入no",
      "value": "no"
    },

    "ProxySite": {
      "description": "填写网站地址即可反代网站，例如：mirror.umd.edu；不填写表示使用默认静态页面。",
      "value": "",
      "required": false
    },

    "UUID": {
      "description": "Vless UUID",
      "value": "fd220b15-d733-40cf-8991-647f00c23557"
    },

    "Xray_Path": {
      "description": "Path路径，可修改。如果开启反代，不要与反代网站存在的uri相同",
      "value": "/1b03a0"
    },
    
    "Share_Path": {
      "description": "分享路径：/Ml2KagRBKXAGQZg/index.html（订阅链接）；/Ml2KagRBKXAGQZg/vless.png（二维码）",
      "value": "/Ml2KagRBKXAGQZg"
    },

    "VER": {
       "description": "Xray版本，默认latest安装最新版本，可以去https://github.com/XTLS/Xray-core/releases选择版本 （例如：1.4.2)",
       "value": "latest"
     }
  },
  "website": "",
  "repository": "",
  "stack": "container"
 }
